{
  "name": "@mayanfinance/wormhole-sdk-route",
  "version": "0.1.3",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "",
  "description": "This package implements an AutomaticRoute for use with the Wormhole Typescript SDK",
  "files": [
    "dist"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "publish": "npm publish --access public --tag $TAG",
    "pub:beta": "TAG=beta npm run publish",
    "pub:latest": "TAG=latest npm run publish",
    "docs": "typedoc",
    "cache": "npx tsx scripts/cache_tokens.ts",
    "example:swap": "npx tsx examples/swap.ts"
  },
  "dependencies": {
    "@mayanfinance/swap-sdk": "7.2.0",
    "@solana/web3.js": "^1.91.7",
    "@wormhole-foundation/sdk-connect": "^0.7",
    "@wormhole-foundation/sdk-evm": "^0.7",
    "@wormhole-foundation/sdk-solana": "^0.7",
    "axios": "^1.4.0",
    "ethers": "^6"
  },
  "peerDependencies": {
    "@wormhole-foundation/sdk-connect": "^0.7",
    "@wormhole-foundation/sdk-evm": "^0.7",
    "@wormhole-foundation/sdk-solana": "^0.7"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "dotenv": "^16.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
