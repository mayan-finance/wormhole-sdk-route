{
  "name": "@mayanfinance/wormhole-sdk-route",
  "version": "0.0.2-beta.3",
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "author": "",
  "description": "The core package for the Connect SDK, used in conjunction with 1 or more of the chain packages",
  "files": [
    "dist"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:cjs && npm run build:esm",
    "publish": "npm publish --access public --tag $TAG",
    "pub:beta": "TAG=beta npm run publish",
    "pub:latest": "TAG=latest npm run publish",
    "docs": "typedoc",
    "example:swap": "tsx examples/swap.ts"
  },
  "dependencies": {
    "@mayanfinance/swap-sdk": "github:artursapek/swap-sdk#mayan-ethers-6",
    "@solana/web3.js": "^1.89.1",
    "@wormhole-foundation/connect-sdk": "0.4.0-beta.9",
    "@wormhole-foundation/connect-sdk-evm": "0.4.0-beta.9",
    "@wormhole-foundation/connect-sdk-solana": "0.4.0-beta.9",
    "axios": "^1.4.0",
    "ethers": "^6"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "dotenv": "^16.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
